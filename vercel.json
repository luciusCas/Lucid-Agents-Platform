{
  "version": 2,
  "buildCommand": "pnpm install --prefer-offline && tsc -b && vite build",
  "outputDirectory": "dist",
  "installCommand": "pnpm install --prefer-offline",
  "framework": "vite",
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
